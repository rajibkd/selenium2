<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Tsunami on Chrome Firefox">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnv" value="Browserstack"/>

    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="resolution" value="1024x768"/>

    <parameter name="url" value="http://www.amazon.com"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="56"/>
    <parameter name="testName" value="TestAmazonApp"/>

    <test name = "Page">
        <classes>
            <class name="Page.TestDropDownAllSearch"/>
        </classes>
    </test>
</suite>