<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Potbelly Client on Chrome Browser">
    <parameter name="url" value="https://www.facebook.com/"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="50"/>
    <test name = "Facebook Home Page">
        <classes>
            <class name="Page.TestFacebookHomepage"/>
            <class name="Page.TestLoginPage"/>
        </classes>
    </test>
</suite>